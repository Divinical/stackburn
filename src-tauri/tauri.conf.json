{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "longDescription": "AI-powered desktop app that scans your digital stack and detects bloat across Google Drive, local folders, and GitHub repositories.",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "StackBurn - Post-AI Survival Weapon",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "StackBurn",
  "mainBinaryName": "StackBurn",
  "version": "1.0.0",
  "identifier": "com.stackburn.desktop",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "resizable": true,
        "title": "StackBurn - Post-AI Survival Weapon",
        "width": 1200,
        "minWidth": 800,
        "minHeight": 600,
        "decorations": true,
        "transparent": false,
        "alwaysOnTop": false,
        "center": true,
        "useHttpsScheme": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}